{
    "name": "hgsgtk/php-cdp",
    "description": "Chrome DevTools Protocol binding for PHP",
    "keywords": [
        "php",
        "cdp",
        "Chrome Devtools Protocol"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kazuki Higashiguchi",
            "email": "hgsgtk@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "PhpCdp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.4",
        "ext-curl": "*",
        "textalk/websocket": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "tests": "phpunit tests"
    }
}
